{
  "name": "furniture-connect-website-t1",
  "version": "0.1.0",
  "private": true,
  "description": "Accountless drop-and-log furniture diversion MVP built with a Next.js-inspired architecture.",
  "scripts": {
    "dev": "node server.js",
    "build": "next build && tsc",
    "start": "NODE_ENV=production node server.js",
    "kill-port": "powershell -Command \"Get-Process -Id (Get-NetTCPConnection -LocalPort 3000).OwningProcess | Stop-Process -Force\"",
    "test": "npm run build && node --test dist/tests/**/*.js",
    "lint": "node scripts/lint-stub.js",
    "format": "node scripts/format-stub.js",
    "dev:stub": "node scripts/dev-stub.js"
  },
  "keywords": [
    "furniture",
    "diversion",
    "mvp"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "next": "^14.2.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.0",
    "dotenv": "^16.3.1",
    "@vercel/edge": "^1.1.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
